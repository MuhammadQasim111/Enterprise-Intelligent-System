{
    "rewrites": [
        {
            "source": "/api/resend/:path*",
            "destination": "https://api.resend.com/:path*"
        },
        {
            "source": "/api/groq/:path*",
            "destination": "https://api.groq.com/:path*"
        },
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}